{
    "cross_validation": {"n_splits":  2},
    "data": {
        "path": "data_1Sep2.feather",
        "target": "f",
        "features": ["f15", "theta15", "N2", "lat", "lon", "h_meas","d15", "e0.0", "e0.5", "e2.0", "e4.5", "e8.0", "e12.5", "e18.0", "e24.5", "e32.0", "e_std"]
    },
    "scaling": {
        "standard": ["f15", "theta15", "N2"],
        "minmax": ["lat", "lon", "h_meas", "e0.0", "e0.5", "e2.0", "e4.5", "e8.0", "e12.5", "e18.0", "e24.5", "e32.0", "station_ri", "e_std"],
        "circular": ["d15"]
