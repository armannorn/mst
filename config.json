{
    "cross_validation": {"n_splits":  2},
    "data": {
        "path": "21Sep.feather",
        "target": "f",
        "features":
        {
            "location": true,
            "predictions": ["f15", "p15", "t15"],
            "elevation": 20000,
            "various": ["N2", "ocean_wind", "TRI"]
        }
    },
    "scaling": {
        "standard": ["predictions", "N2"],
        "minmax": ["location", "elevation", "TRI"]
    },
    "architecture": {
        "layers": [
            {
                "type": "dense",
                "units": 128,
                "activation": "relu"
            },
            {
                "type": "dropout",
                "rate": 0.1
            },
            {
                "type": "dense",
                "units": 128,
                "activation": "relu"
            },
            {
                "type": "dropout",
                "rate": 0.1
            },
            {
                "type": "dense",
                "units": 128,
                "activation": "relu"
            },
            {
                "type": "dropout",
                "rate": 0.1
            },
            {
                "type": "Dense",
                "units": 128,
                "activation": "linear"
            }
        ],
        "output_activation": "linear"
    },
    "training": {
        "optimizer": "adam",
        "learning_rate": 0.0005,
        "loss": "mae",
        "epochs": 100,
        "validation_split": 0.15,
        "test_split": 0.15,
        "batch_size": 128
    },
    "note": "f15+pred+elevation+location+extra",
    "png_name": true
}