{
    "cross_validation": {"n_splits":  3},
    "data": {
        "path": "5oct.feather",
        "utility": 1,
        "target": "f",
        "features":
        {
            "location": true,
            "predictions": ["f15", "p15", "t15"],
            "elevation": 0,
            "stations": false,
            "various": []
        }
    },
    "scaling": {
        "standard": ["predictions", "N2"],
        "minmax": ["location", "elevation", "TRI", "min_dist_to_ocean"]
    },
    "architecture": {
        "layers": [
            {
                "type": "dense",
                "units": 256,
                "activation": "relu"
            },
            {
                "type": "dropout",
                "rate": 0.1
            },
            {
                "type": "dense",
                "units": 256,
                "activation": "relu"
            },
            {
                "type": "dropout",
                "rate": 0.1
            },
            {
                "type": "dense",
                "units": 256,
                "activation": "relu"
            },
            {
                "type": "dropout",
                "rate": 0.1
            },
            {
                "type": "dense",
                "units": 256,
                "activation": "linear"
            }
        ],
        "output_activation": "linear"
    },
    "training": {
        "optimizer": "adam",
        "learning_rate": 0.0005,
        "loss": "mae",
        "epochs": 100,
        "validation_split": 0.15,
        "test_split": 0.15,
        "batch_size": 128,
        "early_stopping": {
            "use": true,
            "patience": 10,
            "verbose": 1,
            "restore": true
        }
    },
    "note": "baseline+pred+location",
    "png_name": true
}