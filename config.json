    "architecture": {
        "layers": [
            {
                "type": "dense",
                "units": 128,
                "activation": "relu"
            },
            {
                "type": "dropout",
                "rate": 0.1
            },
            {
                "type": "dense",
                "units": 128,
                "activation": "relu"
            },
            {
                "type": "dropout",
                "rate": 0.1
            },
            {
                "type": "dense",
                "units": 128,
                "activation": "relu"
            },
            {
                "type": "dropout",
                "rate": 0.1
            },
            {
                "type": "Dense",
                "units": 128,
                "activation": "linear"
            }
        ],
        "output_activation": "linear"
    },
    "training": {
        "optimizer": "adam",
        "learning_rate": 0.0005,
        "loss": "mae",
        "epochs": 100,
        "validation_split": 0.15,
        "test_split": 0.15,
        "batch_size": 128
